fn double(v: i32) -> i32
{
    return %mul(v, 2i32);
}

fn halve(v: i32) -> i32
{
    return %div(v, 2i32);
}

# A pretty normal function to test
pub fn test(m: i32, x: i32, b: i32) -> (i32, i32)
{
    let y1 = %add(%mul(m, x), b);
    let y2 = y1;
    {
        y2 = halve(y2);
    }        
    return (y1, double(y2));
}
